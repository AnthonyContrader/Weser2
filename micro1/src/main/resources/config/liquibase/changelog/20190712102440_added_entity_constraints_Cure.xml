<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Cure.
    -->
    <changeSet id="20190712102440-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="reading_id"
                                 baseTableName="cure"
                                 constraintName="fk_cure_reading_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reading"/>

        <addForeignKeyConstraint baseColumnNames="patology_id"
                                 baseTableName="cure"
                                 constraintName="fk_cure_patology_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patology"/>

    </changeSet>
</databaseChangeLog>
